{
  "name": "harder_better_faster_stronger_vue",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite serve --mode development",
    "test": "",
    "test:unit": "jest",
    "test:e2e": "",
    "prod": "vite serve --mode production",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build:prod": "vue-tsc --noEmit && vite build --mode production",
    "eslint": "eslint . --ext .ts,.vue --fix",
    "stylelint": "stylelint \"**/*.{scss,vue}\" --color --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "**/*.{html,json,yaml,md,ts,scss,vue}": [
      "prettier . --write"
    ],
    "**/*.{ts,vue}": [
      "eslint . --ext .ts,.vue --fix"
    ],
    "**/*.{scss,vue}": [
      "stylelint \"**/*.{scss,vue}\" --color --fix"
    ]
  },
  "dependencies": {
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/test-utils": "^2.0.0-rc.17",
    "autoprefixer": "^10.4.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "lint-staged": "^11.2.6",
    "postcss": "^8.3.11",
    "postcss-html": "^1.2.0",
    "postcss-import": "^14.0.2",
    "postcss-reporter": "^7.0.4",
    "postcss-sorting": "^7.0.1",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-hudochenkov": "^6.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vue-eslint-parser": "^8.0.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.3.0"
  }
}
